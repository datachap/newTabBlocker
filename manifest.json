{
    "manifest_version": 3,
    "name": "newTabBlocker",
    "author": "datachap",
    "version": "1.0",
    "description": "A simple chrome extension to block all new tabs created in a page.",
    "permissions": [
      "webNavigation",
      "scripting",
      "storage"
    ],
    "action": {},
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "background": {
      "service_worker": "background-wrapper.js"
    },
    "content_scripts": [
      {
        "matches": [
          "http://*/*",
          "https://*/*"
        ],
        "js": [
          "tab_purger.js"
        ],
        "run_at": "document_idle"
      }
    ]
  }
  